{
  "dashboard": {
    "title": "ML Platform - Evaluation & Tuning Performance",
    "tags": ["ml", "performance", "evaluation", "tuning"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Active Training Tasks",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "celery_active_tasks{task_name=~\".*train_model.*\"}",
            "legendFormat": "{{task_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Active Tasks"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Active Tuning Tasks",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "celery_active_tasks{task_name=~\".*tune_hyperparameters.*\"}",
            "legendFormat": "{{task_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Active Tasks"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Training Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(model_training_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model_type}} - {{task_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Tuning Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(hyperparameter_tuning_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model_type}} - {{tuning_method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_memory_usage_bytes{process_type=\"celery_worker\"}",
            "legendFormat": "{{process_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "process_cpu_usage_percent{process_type=\"celery_worker\"}",
            "legendFormat": "{{process_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU %"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Task Success Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(celery_task_total{status=\"success\"}[5m]) / rate(celery_task_total[5m]) * 100",
            "legendFormat": "{{task_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "API Request Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket{endpoint=~\".*evaluation.*|.*tuning.*\"}[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Training Samples Distribution",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(model_training_samples_bucket[5m])",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "Tuning Best Scores",
        "type": "graph",
        "gridPos": {"x": 12, "y": 32, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "hyperparameter_tuning_best_score",
            "legendFormat": "{{model_type}} - {{tuning_method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Score", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 11,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"x": 0, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(evaluation_cache_total{result=\"hit\"}[5m]) / rate(evaluation_cache_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "unit": "percent"
        }
      },
      {
        "id": 12,
        "title": "Models Trained (24h)",
        "type": "stat",
        "gridPos": {"x": 6, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(models_trained_total[24h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      },
      {
        "id": 13,
        "title": "Failed Tasks (1h)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(celery_task_total{status=\"failure\"}[1h])",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 14,
        "title": "Queue Length",
        "type": "stat",
        "gridPos": {"x": 18, "y": 40, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "celery_queue_length",
            "legendFormat": "{{queue_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        }
      }
    ]
  }
}
